#!/usr/bin/env python
# -*- coding: utf-8 -*-
import re
import sys
from pathlib import Path
levels_up = 3
path = Path(__file__).resolve().parent
parent_dirs = [(path:=getattr(path, 'parent')) for _ in range(levels_up)]
_ = [sys.path.insert(0, str(parent)) for parent in parent_dirs]
from charset_normalizer.cli.normalizer import cli_detect
if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw|\.exe)?$', '', sys.argv[0])
    sys.exit(cli_detect())
